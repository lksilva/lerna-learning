{
  "name": "elements",
  "description": "Lib UI Elements",
  "version": "1.0.0",
  "author": "Lucas Silva",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.js'",
    "del-dist": "del-cli dist",
    "prebuild": "yarn del-dist",
    "build": "babel src -d dist --ignore test.js,story.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.x",
    "react-dom": "^16.x"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-knobs": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addon-notes": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^1.1.11",
    "glob-loader": "^0.3.0",
    "jest": "23.6.0",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.2",
    "sass-loader": "^7.1.0",
    "storybook-readme": "^4.0.2",
    "style-loader": "^0.21.0"
  },
  "jest": {
    "setupFiles": [
      "./config/jest/test-shim.js",
      "./config/jest/test-setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/config/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true
  },
  "dependencies": {
    "@material-ui/core": "^3.7.1",
    "node-sass": "^4.11.0",
    "styled-components": "^4.1.3"
  }
}
